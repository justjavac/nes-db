{
  "name": "nes-db",
  "description": "NES JSON data. Search and get game info!",
  "version": "1.0.0",
  "author": "justjavac <justjavac@gmail.com>",
  "scripts": {
    "build": "tsc && rollup -c",
    "prepublishOnly": "npm run build",
    "pretty": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "nes",
    "game",
    "play",
    "database"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/justjavac/nes-db"
  },
  "main": "./dist/index.cjs",
