    "types"
  ],
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.10.1",
    "prettier": "^3.1.0",
    "rollup": "^4.6.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "fuzzysort": "^2.0.4"
  }
}
